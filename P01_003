#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
    int num1, num2, f1,f2,f3,Re;
    scanf("%d %d", &num1, &num2);
    f1 = num1 * (num2 % 10);
    f2 = num1 * (num2 / 10 % 10);
    f3 = num1 * (num2 / 100);
    Re = 100 * f3 + 10 * f2 + f1;
    printf("%d\n%d\n%d\n%d", f1, f2, f3, Re);

    return 0;
}
